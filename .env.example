# BlueSavings Environment Configuration
# Copy this file to .env and fill in the values

# Deployment Keys (NEVER commit actual values)
PRIVATE_KEY=your_deployer_private_key_here

# RPC Endpoints (optional - defaults in foundry.toml)
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Block Explorer API Keys
BASESCAN_API_KEY=your_basescan_api_key_here

# Optional: Etherscan API for verification on other chains
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Contract Addresses (filled after deployment)
SAVINGS_VAULT_ADDRESS=
TOKEN_SAVINGS_VAULT_ADDRESS=
YIELD_MANAGER_ADDRESS=
RECURRING_DEPOSIT_ADDRESS=
